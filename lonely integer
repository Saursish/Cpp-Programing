#include <bits/stdc++.h>
using namespace std;
int lonelyinteger(vector<int> a) {
    int i;
    sort(a.begin(),a.end());
    for(i=1;i<a.size()-1;i++)
    {
        if(a[i]!=a[i-1]&&a[i]!=a[i+1])
        {
            break;
        }
    }
    return a[i];
}
int main()
{
    int n,j;
    cin >>n;
    vector<int>a(n);
    for(int i=0;i<n;i++)
    {
        cin>>j;
        a.push_back(j);
    }
    int result=lonelyinteger(a);
    cout<<result<<endl;
}
