#include <bits/stdc++.h>

using namespace std;

int sockMerchant(int n, vector<int> arr) {
    int count_pair=0;
    vector<int> arr2(101,0);
    for(int i:arr)
    {
        arr2[i]++;
    }
    for(int i=0;i<101;i++)
    {
        count_pair=count_pair+arr2[i]/2;
    }
    return count_pair;
}

int main()
{

    int n;
    cin>>n;
    vector<int> ar;

    int ar_item;
    for (int i = 0; i < n; i++) {
        cin>>ar_item;
        ar.push_back(ar_item);
    }

    int result = sockMerchant(n, ar);
    cout << result << "\n";
    return 0;
}
