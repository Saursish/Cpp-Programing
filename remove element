class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int count=nums.size();
        vector<int> v1;
        for(int i=0;i<count;i++)
        {
            if(val!=nums[i])
            {
                v1.push_back(nums[i]);
            }
        }
        v1.swap(nums);
        return nums.size();
    }
};
