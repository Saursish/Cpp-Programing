#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;
class Person{
    protected:
    char name[100];
    int age;
    public:
    virtual void getdata(){
        cin>>name>>age;
    }
    virtual void putdata(){
        cout<<name<<" "<<age<<endl;
    }
};
class Professor:public Person{
    public:
    static int idt;
    Professor(){
        idt++;
    }
    int pub;
    int profid = idt;
    virtual void getdata()
    {
        cin>>name>>age>>pub;
    }
    virtual void putdata(){
        cout<<name<<" "<<age<<" "<<pub<<" "<<profid<<endl;;
    }
};
int Professor::idt=1;
class Student:public Person{
    public:
    static int ids;
    Student(){
        ids++;
    }
    int studid = ids;
    int a[6];
    void getdata(){
        cin>>name>>age>>a[0]>>a[1]>>a[2]>>a[3]>>a[4]>>a[5];
    }
    void putdata(){
        cout<<name<<" "<<age<<" "<<(a[0]+a[1]+a[2]+a[3]+a[4]+a[5])<<" "<<studid<<endl;
    }    
};
int Student::ids=1;
int main(){

    int n, val;
    cin>>n; //The number of objects that is going to be created.
    Person *per[n];

    for(int i = 0;i < n;i++){

        cin>>val;
        if(val == 1){
            // If val is 1 current object is of type Professor
            per[i] = new Professor;

        }
        else per[i] = new Student; // Else the current object is of type Student

        per[i]->getdata(); // Get the data from the user.

    }

    for(int i=0;i<n;i++)
        per[i]->putdata(); // Print the required output for each object.

    return 0;

}
