#include <iostream>
#include <deque>
#include<algorithm>
using namespace std;

void printKMax(int arr[], int n, int k){
    deque<int>ans;
    int i;
    for(i=0;i<k;i++)
    {
        while(!ans.empty()&&arr[i]>=arr[ans.back()])
        ans.pop_back();
        ans.push_back(i);
    }
    for(;i<n;i++)
    {
        cout<<arr[ans.front()]<<" ";
        while(!ans.empty()&&ans.front()<=i-k)
        ans.pop_front();
        while(!ans.empty()&&arr[i]>=arr[ans.back()])
        ans.pop_back();
        ans.push_back(i);
    }
    cout<<arr[ans.front()]<<" ";
}

int main(){
  
	int t;
	cin >> t;
	while(t>0) {
		int n,k;
    	cin >> n >> k;
    	int i;
    	int arr[n];
    	for(i=0;i<n;i++)
      		cin >> arr[i];
    	printKMax(arr, n, k);
        cout<<endl;
    	t--;
  	}
  	return 0;
}
