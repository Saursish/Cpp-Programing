#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n,a;
    cin>>n;
    vector<int> v;
    for(int i=0;i<n;i++)
    {
        cin>>a;
        v.push_back(a) ;  
    } 
    cin>>n;
    v.erase(v.begin()+n-1);
    cin>>n>>a;
    v.erase(v.begin()+n-1,v.begin()+a-1);
    cout<<v.size()<<endl;
    for(int i:v)
    {
        cout<<i<<" ";
        i++;
    }
    return 0;
}
